<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jzy_3d_sample.ui.RcslistController">
   <children>
      <HBox layoutX="15.0" layoutY="88.0">
         <children>
            <Label prefHeight="25.0" prefWidth="50.0" text="RCS 檔案">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="RCSFileText" prefHeight="25.0" prefWidth="460.0" />
            <Button fx:id="RCSFileButton" mnemonicParsing="false" onAction="#RCSFileButtonAction" prefHeight="25.0" prefWidth="41.0" text="...">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <HBox layoutX="493.0" layoutY="163.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onOk" text="OK">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onCancel" text="Cancel" />
         </children>
      </HBox>
   </children>
</AnchorPane>
