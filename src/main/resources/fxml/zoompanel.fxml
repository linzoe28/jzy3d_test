         <children>
            <Slider fx:id="zoomSlider" blockIncrement="1.0" majorTickUnit="7.0" max="16.0" min="1.0" minorTickCount="6" orientation="VERTICAL" showTickLabels="true" showTickMarks="true" snapToTicks="true" value="1.0" />
