      <Slider fx:id="ySlider" blockIncrement="1.0" layoutX="41.0" layoutY="277.0" majorTickUnit="20.0" minorTickCount="20" orientation="VERTICAL" showTickLabels="true" showTickMarks="true" value="50.0" />
      <Slider fx:id="zSlider" blockIncrement="1.0" layoutX="78.0" layoutY="277.0" majorTickUnit="20.0" minorTickCount="20" orientation="VERTICAL" showTickLabels="true" showTickMarks="true" value="50.0" />
